a,a:active,a:focus,a:hover{outline:0;text-decoration:none;color:#444}input:-moz-placeholder,input::-webkit-input-placeholder,textarea:-moz-placeholder,textarea::-webkit-input-placeholder{color:#D4D4D4}button,input{font-family:"Microsoft YaHei";border:none;overflow:visible}textarea{margin:0;padding:0;resize:none}.btn.active.focus,.btn.active:focus,.btn.focus,.btn:active.focus,.btn:active:focus,.btn:focus{outline:0}dd,dl,h1,h2,h3,h4,h5,h6,li,p,ul{list-style-type:none;padding:0;margin:0;font-weight:400}em,i{font-style:normal}button{border:none}input:-webkit-autofill{-webkit-box-shadow:0 0 0 1000px #fff inset}button,input,select,textarea{outline:0}a{-moz-transition:all .3s ease;-ms-transition:all .3s ease;-o-transition:all .3s ease;transition:all .3s ease}body{background:#f7f9fa;color:#444;font:14px/1.5 tahoma,arial,'Hiragino Sans GB','microsoft yahei'}input.disable{background-color:#eee!important}